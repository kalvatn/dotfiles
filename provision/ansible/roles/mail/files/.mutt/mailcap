text/*; vim %s;
image/*; feh %s; test=sh -c 'test $DISPLAY';
application/pdf; background-viewer xdg-open %s; test=sh -c 'test $DISPLAY';
application/octet-stream; vim %s;
application/*; background-viewer xdg-open %s; test=sh -c 'test $DISPLAY';
